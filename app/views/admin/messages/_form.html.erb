<%= form_for [:admin, @message] do |f| %>

	<%= render 'error_messages', object: f.object %>

	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :date %>
	<%= f.date_select :date %>

	<%= f.label :speaker %>
	<%= f.text_field :speaker %>

	<%= f.label :file %>
	<%= f.file_field :file %>

	<%= f.submit %>
<% end %>